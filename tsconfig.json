{
  "compilerOptions": {
    "target": "ES2015",
    "module": "es2015",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "moduleResolution": "node",
    "pretty": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "declarationDir": "dist",
    "outDir": "./dist",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "allowJs": true,
    "jsx": "react",
    "sourceMap": true,
    "compileOnSave":true,
    "inlineSourceMap": true,
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "importHelpers": true,
    "skipLibCheck": true,
    "isolatedModules": false,
    "baseUrl": ".",
    "paths": {
      "utils_study": ["./"]
    },

  },
  "exclude": [
    "node_modules",
    "dist",
  ]
}

